{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceRoot}/src",
      "userDataDir": "${workspaceRoot}/.vscode/chrome",
      "sourceMapPathOverrides": {"webpack:///src/*": "${webRoot}/*"}
    }/*,
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceRoot}/target/main.js",
      "sourceMaps": true,
      "outFiles": ["${workspaceRoot}/target/*.js"]
    }*/
  ]
}